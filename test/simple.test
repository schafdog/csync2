daemon d
clean local
clean peer
FILENAME="new_file 'N' all"
mkdir -p "test/$NAME"
echo "far" > "test/$NAME/$FILENAME"
./portable_touch.sh "test/$NAME/A"
./portable_touch.sh "test/$NAME/.Test"
cmd c "New $NAME/$FILENAME"
cmd M "New $NAME/$FILENAME"
cmd x "New $NAME/$FILENAME"
cmd T test/$NAME "List daemon"
sleep 1
./portable_touch.sh "test/$NAME/mark"
cmd m "test/$NAME/mark (mark)"
cmd u "test/$NAME/mark (sync)"
sleep 2
echo "vel" > "test/$NAME/$FILENAME"
cmd c "updated $NAME/$FILENAME (MOD)"
cmd u "updated $NAME/$FILENAME"
rm -rf test/$NAME
cmd x "clean up"
killdaemon
